<h1>Create your user profile!</p>
  <%= form_for User.new do |f| %>
  <%= f.label(:user_name) %><br>
  <%= f.text_field(:user_name, placeholder: 'Name')%><br>
  <%= f.label(:email) %><br>
  <%= f.text_field(:email, placeholder: 'Email')%><br>
  <%= f.label(:password_digest) %><br>
  <%= f.password_field(:password_digest, placeholder: 'Password')%><br>
  <%= f.submit('Submit') %>
<% end %>
