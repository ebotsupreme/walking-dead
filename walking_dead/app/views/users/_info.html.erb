<div id="edit_profile">
  <h1>Edit your profile</h1>
  <p>All  your edits are belong to us!</p>
</div>

<div class="jumbotron col-md-3 col-md-offset-0">
  <%= form_for @user, html: { multipart: true } do |f| %>
    <%= f.label(:user_name) %><br>
    <%= f.text_field(:user_name, placeholder: 'Name')%><br>
    <%= f.label(:email) %><br>
    <%= f.text_field(:email, placeholder: 'Email')%><br>
    <%= f.label(:bio) %><br>
    <%= f.text_field(:bio, placeholder: 'Bio')%><br>
    <%= f.label(:image) %><br>
    <%= f.file_field(:image)%><br>
    <%= f.submit('Submit') %>
  <% end %>
</div>
